    "start": "ng serve",
    "start:ssl": "ng serve --ssl --live-reload false",
    "build": "ng build",
    "build:packagr": "ng-packagr -p package.json",
    "publish:dist": "cd ./dist && npm publish --access=public",
    "test": "ng test --watch=false",
