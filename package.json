    "@angular/platform-browser-dynamic": "~10.0.0",
    "@angular/router": "~10.0.0",
    "core-js": "^3.6.5",
    "crypto-js": "3.1.9-1",
    "ng-cli-pug-loader": "^0.2.2",
    "rxjs": "6.5.2",
    "rxjs-compat": "6.5.5",
    "zone.js": "~0.10.3"
  },
